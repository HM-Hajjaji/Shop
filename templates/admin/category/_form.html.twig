{{ form_start(form)}}
<div class="form-group">
    {{ form_row(form.name,{'attr': {'class': 'form-control','placeholder' : 'Enter Name'}}) }}
</div>
<div class="form-group ">
    {{ form_row(form.description,{'attr': {'class': 'form-control','style' : "height: 158px;",'placeholder' : 'Enter Description'}}) }}
</div>
{{ form_row(form.submit,{'attr':{'class': 'btn btn-success mr-1'}}) }}
    {{ form_end(form) }}